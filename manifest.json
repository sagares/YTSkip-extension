{
  "manifest_version": 2,
  "name": "Skip YT Video",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["/js/content-script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "youtube-skip.png",
    "default_title": "Skipper",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "<all_urls>",
    "activeTab"
  ]
}